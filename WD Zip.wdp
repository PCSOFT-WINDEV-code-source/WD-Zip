     internal_properties : CAAAAAgAAABGj7hefZCKMVkOMZa7tErZEnxH+0WcpQT8eBKbC2A2S/xWlRiojg7f9YV1yyikOhH/tEiN8lcu1u6Z+6/JQpitdK7cjzfB4cf1kzIkOY7rZIZs2vvKBCr/h38WPwmuXwzQa98bxnDsCWvn53a6MkJT1KfIvawb339e5BefoAgrafqxDvM4kMcEW8igb4pqURMC
     code : |1+
      EXTERN "WINCONST.WL"
      
      GLOBAL
      	gMonArchive est une zipArchive 
      	gsBaseDeRegistre	est une chaîne
      	gsArchive			est une chaine
      	geTraitement est une EAction
      	gbAffichageModeRépertoire est un booléen
      	
      	ERetourOUINON est une énumération
      		OUI_POUR_1
      		NON_POUR_1
      		OUI_POUR_TOUS
      		NON_POUR_TOUS
      		ANNULER_TRAITEMENT
      	FIN
      	
      	EAction est une énumération
      		Ouvrir = "Ouvrir l'archive"
      		Extraire_ici = "Extraire ici"
      		Extraire_rep = "Extraire dans le dossier"
      		AjouterFile_choisir_archive = "Ajouter à l'archive..."
      		Compress_wdz = "Compresser en .wdz"
      		Compress_zip = "Compresser en .zip"
      	FIN
      	
      	
      LOCAL
      	
      
      	
      // Créer la clé dans la base de registre
      gsBaseDeRegistre = ProjetInfo(piRegistre)
      RegistreCréeClé(gsBaseDeRegistre)
      
      
      
      // On vérifie si on est en mode test
      si EnModeTest() = faux et EnModeTestAutomatique() = Faux ALORS
      	// On vérifie si l'on a besoin de lancer l'appli en mode administrateur pour les menus du programme
      	SI ChargeParamètre("RegistreOK",Faux)=vrai ALORS
      				
      		si Association_Complete() =faux ALORS
      			Dissociation_Complete()
      			FinProgramme()
      		FIN
      			
      		// On sauvegarde le fait que l'application ne soit plus associées aux fichiers d'archive.
      		SauveParamètre("RegistreOK",Faux)
      		SauveParamètre("EtatRegistre",vrai)
      		
      		FinProgramme()
      	FIN
      FIN
      
      
      
      // Doit on supprimer l'association des fichiers d'archive à l'application WD Zip
      SI  ChargeParamètre("SupprimerRegistre",Faux)=vrai ALORS
      	
      	Dissociation_Complete()
      	
      	// On sauvegarde le fait que l'application ne soit plus associées aux fichiers d'archive.
      	SauveParamètre("SupprimerRegistre",faux)
      	SauveParamètre("EtatRegistre",faux)
      	
      	FinProgramme()
      FIN
      
      
      
      // On déclare toutes les actions possibles
      e1 est une EAction = Ouvrir
      e2 est une EAction = Extraire_ici
      e3 est une EAction = Extraire_rep 
      e4 est une EAction = AjouterFile_choisir_archive
      e5 est une EAction = Compress_zip
      e6 est une EAction = Compress_wdz
      
      
      // On vérifie si l'application est appelé avec des paramètres d'action
      si LigneCommande(1) <> "" ALORS
      	
      	// Selon l'action choisie, on ouvre la fenêtre avec le traitement nécessaire (geTraitement = action demandée pour FEN_ATTENTE_TRAITEMENT)
      	selon LigneCommande(1)
      	
      		cas MiseEnFormeParamètre(e1..valeur)
      			// Le deuxième paramètre de la ligne de commande contient la valeur du fichier à ouvrir
      			ouvre(FEN_PRINCIPALE,2)
      			
      		// Cas ou on ne passe pas par l'interface principale
      		CAS MiseEnFormeParamètre(e2..Valeur)
      			geTraitement = e2
      			Ouvre(FEN_ATTENTE_TRAITEMENT,Faux)
      			                                 
      		CAS MiseEnFormeParamètre(e3..Valeur) 
      			geTraitement=e3                  
      			Ouvre(FEN_ATTENTE_TRAITEMENT,Faux)
      			                                 
      		CAS MiseEnFormeParamètre(e4..Valeur) 
      			geTraitement=e4                  
      			Ouvre(FEN_ATTENTE_TRAITEMENT,Faux)
      			                                 
      		CAS MiseEnFormeParamètre(e5..Valeur) 
      			geTraitement=e5                  
      			Ouvre(FEN_ATTENTE_TRAITEMENT,Faux)
      			                                 
      		CAS MiseEnFormeParamètre(e6..Valeur)
      			geTraitement=e6                  
      			Ouvre(FEN_ATTENTE_TRAITEMENT,Faux)	
      			
      		AUTRE CAS
      			// Le premier paramètre de la ligne de commande contient la valeur du fichier à ouvrir
      			ouvre(FEN_PRINCIPALE,1)	
      	FIN
      sinon
      	Ouvre(FEN_PRINCIPALE,0)
      FIN
