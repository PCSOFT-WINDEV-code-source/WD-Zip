         si TABLE_ARCHIVE[TABLE_ARCHIVE].COL_ICONE.IMG_Fichier = IMG_Repertoire ALORS
         	// Si première ligne => ".." on revient en arrière
         	si TABLE_ARCHIVE = 1 et gnNiveauAffichage>0  ALORS
         		gnNiveauAffichage -- 
         		// On sauvegarde le répertoire courant (c'est à dire qu'on enlève le dernier répertoire de la chaine répertoire courant
         		si contient(gsCheminRepertoireCourant,fsep)= vrai ALORS
         			gsCheminRepertoireCourant = gauche(gsCheminRepertoireCourant,(Taille(gsCheminRepertoireCourant) - taille(ExtraitChaîne(gsCheminRepertoireCourant,1,fSép,DepuisFin))-1))
         		sinon
         			// Si on a pas de fSep alors on est au premier répertoire avant la racine, et par conséquent il faut mettre le chemin à vide
         			gsCheminRepertoireCourant = ""
         		FIN
         	sinon
         		gnNiveauAffichage++
         		// On sauvegarde le répertoire courant
         		gsCheminRepertoireCourant+= [fsep]+TABLE_ARCHIVE[TABLE_ARCHIVE].COL_NOM
         	FIN
         	
         	LIB_RepertoireCourantArchive = gsCheminRepertoireCourant
         	
         	// On rafraîchît l'affichage
         	RafraichitAffichage
         sinon	
         	Ouvrefichier()
         FIN
