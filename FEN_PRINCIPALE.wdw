     name : DragAndDrop
     internal_properties : CAAAAAgAAADdDwpT3O5du/eIl822O4umfvUP6b7PJGONC4dHY4lWrl4NfgtuAtiajXnSXhymxnc71wrSo8IcWgg1zb0KO0Rm5r/3EW7copgcgHu7aAskBs2oERNoAnL1gkCqqJXWvCnoeu5UHzS/hy+p0NwJhN2I1v3dBebp0F3FozfJ46gjr61+gMxApLmmbqg7VgGXwHvYpYvK1FmCne3s/B8VsReU6Cw=
     procedure_id : 1175213845174855448
     type_code : 14
     code : |1+
      // Résumé : Fonction appelée automatiquement lors du drag and drop explorer
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	booléen : // 	Aucune
      //
      Procédure DragAndDrop()
      
      i est un entier
      nNbElements est un entier
      sElément est une chaine
      
      // Récupération du nombre d'éléments sélectionnés (fichiers + répertoires)
      nNbElements = ExplorerRécupère(_EVE.wParam)
      
      // cas particulier 
      si nNbElements=1 ALORS
      	sElément = ExplorerRécupère(_EVE.wParam, 1)
      	
      	// On est dans le drag and drop depuis la table ? 
      	si sElément = COL_DragAndDrop_Explorer.sFichierObserve ALORS
      		COL_DragAndDrop_Explorer.FinDND()
      		renvoyer Faux
      	FIN
      	
      	si fExtraitChemin(sElément,fExtension)~=".wdz" _ou_ fExtraitChemin(sElément,fExtension)~=".zip" _ou_ ...
      				 fExtraitChemin(sElément,fExtension)~=".rar" _OU_ fExtraitChemin(sElément,fExtension)~=".cab" ALORS
      		si zipExiste(gMonArchive)=vrai alors
      			si ouinon(chaineconstruit(<§$0086§>, fExtraitChemin(sElément,fFichier+fExtension)),...
      						 <§$0087§>, <§$0088§>)=oui ALORS
      				FermeArchive()
      				OuvreArchive(sElément)
      				renvoyer vrai
      			FIN
      		sinon
      			OuvreArchive(sElément)
      			renvoyer vrai
      		fin
      	FIN
      FIN
      
      
      // Début du traitement
      sablier(vrai)
      JAUGE_TraitementCourant..Visible =vrai
      JAUGE_TraitementCourant = 0
      
      ListeSupprimeTout(LISTE_CACHE)
      
      // PArcours des éléments (fichiers + répertoires)
      POUR i=1 à nNbElements
      	
      	sElément = ExplorerRécupère(_EVE.wParam, i)
      	
      	// s'il s'agit d'un fichier
      	si fFichierExiste(sElément)=vrai alors
      		listeajoute(LISTE_CACHE,sElément)
      	sinon
      		// liste les fichiers du répertoire	
      		fListeFichier(sElément+fsep+"*.*", "ListeFichiers")
      	FIN
      FIN
      
      // ajoute à l'archive les fichiers contenus dans la liste cachée
      AjouteListeFichier()
      
      // Fin du traitement
      sablier(faux)
      JAUGE_TraitementCourant..Visible =faux
      
      MajEtatBoutons()
      
      renvoyer vrai
