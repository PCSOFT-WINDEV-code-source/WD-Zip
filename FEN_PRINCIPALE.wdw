   -
     name : OuvreArchive
     internal_properties : CAAAAAgAAADLYJcH0zZWac/0rFFB4/2IXPnsp8aXiJngIKMpv84EL+Rl1KwQg0tNN8A6yw5ltdNk+XKxs/wAEfiU1Q4VgOGEmxA98VkkAn/RWvukaogGqSMIbEbWS3UfnOsNyCU/Yz5AY8EA7AzMI2TdNhDR/tXwlUcmwjH7xMcNKDw3MI9VzFvSQY814OIsVy9VNIgbgZWbHE/F0UDY/Jg6Bvwnl74uFOhMV/MenIvgjtzFXdAuEi5xppE=
     procedure_id : 1602347685645335236
     type_code : 14
     code : |1-
      // Résumé : Permet d'ouvrir une archive déjà existante
      // Paramètres :
      //	sArchive (chaîne ANSI) :Chemin de l'archive déjà existante
      // Valeur de retour :
      // 	booléen : Vrai si l'ouverture a réussi, faux sinon
      //
      PROCEDURE OuvreArchive(sArchive est une chaine) : booléen
      
      // On indique à l'utilisateur qu'un traitement est en cours
      Sablier(Vrai)
      JAUGE_TraitementCourant..Visible =Vrai
      JAUGE_TraitementCourant = 0
      
      // Fermer l'éventuelle archive ouverte
      FermeArchive()
      
      // On vérifie si l'archive est en lecture seule
      SI Position(fAttribut(sArchive), "R")>0 ALORS
      	MaFenêtre..Titre += " "+<§@163ca2ca012797500007§>
      FIN
      
      // Echec de l'ouverture ? 
      si ArchiveOuvreArchive(sArchive) = faux ALORS
      	Sablier(Faux)
      	JAUGE_TraitementCourant..Visible =Faux
      	RENVOYER Faux
      FIN
      
      
      // On initialise la jauge indiquant qu'un traitement est en cours 
      JAUGE_TraitementCourant..BorneMax = zipNbFichier(gMonArchive)
      JAUGE_TraitementCourant= 0
      
      // On affiche l'archive
      RafraichitAffichage
      
      // Chemin de l'archive courante
      gsArchive = sArchive
      
      // Mise à jour du titre de la fenêtre
      MaFenêtre..Titre = ProjetInfo(piNomProjet)+" - "+fExtraitChemin(sArchive, fFichier+fExtension)
      
      // On désélectionne la table
      TableSelectMoins(TABLE_ARCHIVE)
      
      // Mise à jour des messages pour l'utilisateur
      LIB_MESSAGE1 = ChaîneConstruit(<§@163ca2ca012797500003§>, gMonArchive.Fichier..Occurrence, TailleVersChaîne(zipTaille(gMonArchive)))
      LIB_MESSAGE2 = ChaîneConstruit(<§@163ca2ca012797500004§>, 0, TailleVersChaîne(0))
      LIB_CheminArchive = Gauche(gsArchive,Position(gsArchive,fSep,Taille(gsArchive),DepuisFin)-1)
      LIB_NomArchive = ExtraitChaîne(gsArchive,1,fSep,DepuisFin)
      
      
      // On accepte pas le drag and drop pour les fichier .rar
      SI gMonArchive.FormatArchive=zipFormatRAR ALORS
      	// Pas de drop depuis l'explorateur Windows
      	ExplorerAccepte(Faux,FEN_PRINCIPALE..Nom)
      	LIB_MESSAGE3 = ""
      SINON
      	// Autre format d'archive
      	// Drop autorisé depuis l'explorateur Windows
      	ExplorerAccepte(Vrai,FEN_PRINCIPALE..Nom)
      	LIB_MESSAGE3 = <§@163ca2ca012797500005§>
      FIN
      
      // Etat des boutons
      MajEtatBoutons()
      
      // On indique que le traitement est terminé
      Sablier(Faux)
      JAUGE_TraitementCourant..Visible =Faux
      
      renvoyer vrai
     type : 458752
   -
     name : AjouteListeFichier
     procedure_id : 1602347805905430314
     type_code : 14
     code : |1-
      // Résumé : Ajoute une liste de fichier à l'archive
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AjouteListeFichier()
      
      i est un entier
      bFichiersCachés, bSauverRep, bCheminAbsolu sont des booleens
      sRetour est une chaine
      nModeAjout est un entier
      sAttribut est une chaine
      nNbFichierAvantAjout est un entier 
      
      
      // L'archive existe ? 
      SI zipExiste(gMonArchive)=Faux ALORS
      	SI OuiNon(<§@163ca2ca012797500000§>)=Oui ALORS
      		ExécuteTraitement(BTN_NOUVEAU, trtClic)
      		SI zipExiste(gMonArchive)=Faux ALORS RETOUR
      	SINON
      		RETOUR
      	FIN
      FIN
      
      
      // Demande du mode d'ajout
      sRetour = Ouvre(FEN_MODE_AJOUT)
      SI sRetour<>"" ALORS
      	bSauverRep = ExtraitChaîne(sRetour,1,TAB)
      	bFichiersCachés = ExtraitChaîne(sRetour,2,TAB)
      	bCheminAbsolu = ExtraitChaîne(sRetour,3,TAB)
      SINON
      	RETOUR
      FIN
      SI bSauverRep ALORS
      	nModeAjout = zipRépertoire
      SINON
      	nModeAjout = zipAucun
      FIN
      SI bCheminAbsolu ALORS
      	nModeAjout = zipDisque
      FIN
      
      
      // On récupère le nombre de fichier déjà existant sur notre archive
      nNbFichierAvantAjout = gMonArchive..NbFichier
      nNbFichierAvantAjout++
      
      tabFichier est un tableau de chaine
      // Pour chaque fichier à ajouter
      POUR i=1 _A_ ListeOccurrence(LISTE_CACHE)
      	
      	SI bFichiersCachés=Vrai ALORS
      		//sListeFichier += [RC] + 
      		TableauAjoute(tabFichier,LISTE_CACHE[i])
      	SINON
      		
      		sAttribut = fAttribut(LISTE_CACHE[i])
      		// les fichier cachés et système ne sont pas ajoutés
      		SI Position(sAttribut,"H")+Position(sAttribut,"S")=0 ALORS
      			TableauAjoute(tabFichier,LISTE_CACHE[i])
      		FIN
      	FIN	
      FIN
      
      // On commence le traitement d'ajout
      Sablier(Vrai)
      JAUGE_TraitementCourant..Visible = Vrai
      JAUGE_TraitementCourant = 0
      
      
      si ArchiveAjouteListeFichier(tabFichier,nModeAjout,JAUGE_TraitementCourant) = Faux ALORS
      	retour
      FIN
      
      TABLE_ARCHIVE..AffichageActif = faux
      POUR i = nNbFichierAvantAjout _a_ gMonArchive.NbFichier
      	AfficheUnFichierArchive(i)	
      FIN
      
      // On rafraîchit l'affichage
      RafraichitAffichage
      
      
      TABLE_ARCHIVE..AffichageActif = vrai
      
      // Fin de l'ajout
      Sablier(Faux)
      JAUGE_TraitementCourant..Visible =Faux
     type : 458752
