      // Résumé : Regroupe des multi-archives en une seule archive
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure RegroupeArchive()
      nRésultat est un entier
      i est un entier
      sTemp est une chaine
      sExtension est une chaine
      sArchiveRegroupée est une chaine
      sArchiveDecoupee est une chaine
      sSOVNom est une chaine
      
      // On vérifie que l'archive n'est pas vide 
      si gsArchive="" ALORS
      	erreur(<§$00a5§>)
      	retour
      FIN
      
      // On vérifie que l'archive est bien multi volumes
      si zipEstMulti(gMonArchive)=faux ALORS
      	erreur(<§$008b§>)
      	retour
      FIN
      
      
      // Début du traitement
      sablier(vrai)
      JAUGE_TraitementCourant..Visible =vrai
      JAUGE_TraitementCourant = 0
      
      // Nom temporaire de l'archive regroupée
      sTemp = fExtraitChemin(gsArchive,fDisque+fRépertoire+fFichier)
      sExtension = fExtraitChemin(gsArchive,fExtension)
      
      sArchiveRegroupée = sTemp+"_r"+sExtension
      
      // Regroupage
      nRésultat = zipRegroupe(gMonArchive,sArchiveRegroupée,JAUGE_TraitementCourant)
      si nRésultat<>0 alors
      	erreur(<§$0075§>, zipMsgErreur(nRésultat))	
      	sablier(faux)
      	JAUGE_TraitementCourant..Visible = faux
      	retour
      fin
      
      sSOVNom = gsArchive
      
      // fermeture de l'archive courante
      FermeArchive()
      
      // Suppression des multi-Archives
      fSupprime(sSOVNom)
      
      // En mode zip et en mode wdz, les fichiers découpés n'ont pas les mêmes extensions (.z01 OU .001)
      si sExtension~=".wdz" alors
      	i=1
      	sArchiveDecoupee = sTemp + "." + NumériqueVersChaine(i,"03d")
      	TANTQUE fFichierExiste(sArchiveDecoupee)
      		fSupprime(sArchiveDecoupee)
      		i++
      		sArchiveDecoupee = sTemp + "." + NumériqueVersChaine(i,"03d")
      	FIN
      sinon
      	i=1
      	sArchiveDecoupee = sTemp + ".z" + NumériqueVersChaine(i,"02d")
      	
      	TANTQUE fFichierExiste(sArchiveDecoupee)
      		fSupprime(sArchiveDecoupee)
      		i++
      		sArchiveDecoupee = sTemp + ".z" + NumériqueVersChaine(i,"02d")
      	FIN
      fin
      
      // Renommage de l'archive avec son nom définitif
      frenomme(sTemp+"_r"+sExtension, sTemp+sExtension)
      
      // Ouverture de l'archive regroupée
      OuvreArchive(sTemp+sExtension)
      
      info(<§$008d§>, gsArchive)
      
      // Etat des boutons
      MajEtatBoutons()
      
      // Fin du traitement
      sablier(faux)
      JAUGE_TraitementCourant..Visible =Faux
