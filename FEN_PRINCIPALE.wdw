      // Résumé : Ferme l'archive actuellement ouverte
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure FermeArchive()
      // Procédure qui permet de fermer l'archive en cours si c'est nécessaire
      
      // Fermer l'éventuelle archive ouverte
      SI zipExiste(gMonArchive)=vrai alors 
      	// Pas de jauge sur le zipFerme on affiche donc une fenêtre d'attente
      	OuvreFille(FEN_ATTENTE_TRAITEMENT,Vrai)
      	
      	// On exécute le zip ferme en parallèle de l'attente
      	TâcheParallèleFermeture est une TâcheParallèle
      	TâcheParallèleFermeture = TâcheParallèleExécute(ZipFermeParallele)
      	TANTQUE TâcheParallèleAttend(TâcheParallèleFermeture,20)=Faux
      		FenRepeint(FEN_ATTENTE_TRAITEMENT)
      	FIN
      	
      	// On ferme la fenêtre d'attente
      	Ferme(FEN_ATTENTE_TRAITEMENT)
      	
      	
      FIN
      
      
      TableSupprimeTout(TABLE_ARCHIVE)
      
      // Ré-initialiser les variables
      gfbExécutable = faux
      gsArchive = ""
      
      // Enlever le message de la barre de message
      LIB_MESSAGE1 = ChaîneConstruit(<§@163ca2ca012797500003§>, 0, TailleVersChaîne(0))
      LIB_MESSAGE2 = ChaîneConstruit(<§@163ca2ca012797500004§>, 0, TailleVersChaîne(0))
      
      // Mettre à jour les différents boutons
      MajEtatBoutons()		
      
      // Mettre à jour le titre de la fenêtre
      mafenetre..titre = ProjetInfo(piNomProjet)
