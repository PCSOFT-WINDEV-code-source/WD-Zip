      // Résumé : Découpe une archive en plusieurs petits volume
      // Paramètres :
      //	psPourPilotage (valeur par défaut="") :Répertoire pour le découpage
      // Valeur de retour :
      // 	Aucune
      //
      Procédure DécoupeArchive(psPourPilotage="")
      nTaille est un entier
      bSupprArchive est un booleen
      sRetour est une chaine
      nRésultat est un entier
      sArchiveDecoupee est une chaine
      sTemp est une chaine
      sExtension est une chaine
      i est un entier
      sSOVNom est une chaine
      
      
      // On vérifie que l'archive existe
      si zipExiste(gMonArchive)=Faux ALORS
      	erreur(<§$00a5§>)
      	retour
      FIN
      
      // Si l'archive est déjà en multi volumes
      si zipEstMulti(gMonArchive)=vrai ALORS
      	erreur(<§$008e§>)
      	retour
      FIN
      
      // Si l'option n'est pas autorisé
      SI gMonArchive.FormatArchive dans (zipFormat7Z, zipFormatCAB, zipFormatRAR, zipFormatTAR, zipFormatTARGZ) ALORS
      	Erreur(<§$00a9§>)
      	RETOUR
      FIN
      
      
      // Répertoire ou découper l'archive
      si psPourPilotage=""
      	sRetour = ouvre(FEN_DECOUPE_ARCHIVE)
      sinon
      	sRetour = psPourPilotage
      fin
      
      
      si sRetour<>"" ALORS
      	// Taille désirée
      	nTaille = ExtraitChaîne(sRetour,1,tab)
      	// demande de suppression de l'archive d'origine
      	bSupprArchive = ExtraitChaîne(sRetour,2,tab)
      	
      	// On vérifie que la taille des sous volumes est assez grande
      	si nTaille<300*1024 ALORS
      		erreur(<§$009d§>)
      		retour
      	FIN
      	
      	// Si pas besoin de découpage
      	si zipNbPartieNécessaire(gMonArchive,nTaille)<=1 ALORS
      		info(<§$008f§>)
      		retour
      	FIN
      	
      	// Début du traitement
      	sablier(vrai)
      	JAUGE_TraitementCourant..Visible =vrai
      	JAUGE_TraitementCourant = 0
      	
      	// Nom temporaire de l'archive découpée
      	sTemp = fExtraitChemin(gsArchive,fdisque+fRépertoire+fFichier)
      	sExtension = fExtraitChemin(gsArchive,fExtension)
      	sArchiveDecoupee = sTemp+"_d"+sExtension
      	
      	// Découpage de l'archive
      	nRésultat = zipDécoupe(gMonArchive, sArchiveDecoupee, nTaille, faux,JAUGE_TraitementCourant)
      	si nRésultat>0 ALORS
      		erreur(<§$0090§>, zipMsgErreur(nRésultat))
      		sablier(faux)
      		JAUGE_TraitementCourant..Visible =faux
      		retour
      	FIN
      	
      	sSOVNom = gsArchive
      	
      	// fermeture de l'archive courante
      	FermeArchive()
      	
      	// Renommage de l'archive d'origine
      	si bSupprArchive=faux ALORS
      		frenomme(sSOVNom, sTemp+"_complete"+sExtension)
      	sinon
      		fsupprime(sSOVNom)
      	FIN
      			
      	// renommage des archives découpées
      	frenomme(sTemp+"_d"+sExtension, sTemp+sExtension)
      	
      	// en mode zip et en mode wdz, les fichiers découpés n'ont pas les mêmes extensions (.z01 OU .001)
      	si sExtension~=".wdz" ALORS
      		i=1; sArchiveDecoupee = sTemp + "_d." + NumériqueVersChaine(i,"03d")
      		tantque fFichierExiste(sArchiveDecoupee)
      			frenomme(sArchiveDecoupee, sTemp +"."+ NumériqueVersChaine(i,"03d"))
      			i++; sArchiveDecoupee = sTemp + "_d." + NumériqueVersChaine(i,"03d")
      		FIN
      	sinon
      		i=1; sArchiveDecoupee = sTemp + "_d.z" + NumériqueVersChaine(i,"02d")
      		tantque fFichierExiste(sArchiveDecoupee)
      			frenomme(sArchiveDecoupee, sTemp +".z"+ NumériqueVersChaine(i,"02d"))
      			i++; sArchiveDecoupee = sTemp + "_d.z" + NumériqueVersChaine(i,"02d")
      		FIN
      	FIN
      	
      	// ouverture de l'archive découpée
      	OuvreArchive(sTemp+sExtension)
      	
      	info(ChaîneConstruit(<§$0076§>,zipNbPartie(gMonArchive)))
      	
      	// Etat des boutons
      	MajEtatBoutons()
      	
      	// FIn du traitement
      	sablier(faux)
      	JAUGE_TraitementCourant..Visible =faux
      FIN
      
