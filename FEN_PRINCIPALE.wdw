     name : SupprimeFichiers
     internal_properties : CAAAAAgAAABp4J5HSCBYXvxvvtv/hChjdyeyUYkf7lpdWj6bMBbfUgOdnLtxtjslF8T5A4dNoHi3G/uNVAXMOym5NS0YTvPBzVfZzECrqBRx5n9qEY47VN6tVz4DOYYHeVOv/088FCMALs9zK5/ncqMnNc7IP2RCcnc0/uS6uFjxx6o/iCGgGNp9LHDtTnCoXz/JKQMjBgqp5PozZrZXiqruHL/TdS1M4kQBJYpE+1/dOIeS9q/OQeP4Mdyqx/vFP6hNAvLkHg==
     procedure_id : 1175213845174789912
     type_code : 14
     code : |1+
      // Résumé : supprimer les fichiers sélectionnés de l'archive
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure SupprimeFichiers()
      bSuppressionConfirmée est un booléen = Faux
      tabFichier est un tableau de chaînes
      sListeFichier est une chaine
      
      
      
      si gsArchive="" ALORS
      	erreur(<§$00a5§>)
      	retour
      FIN
      
      si TableSelectOccurrence(TABLE_ARCHIVE)=0 alors
      	si ouinon(<§$00a7§>)=Oui ALORS
      		TableSelectPlus(TABLE_ARCHIVE)
      		bSuppressionConfirmée = Vrai
      	SINON
      		RETOUR
      	FIN
      fin
      
      Si bSuppressionConfirmée = Faux alors
      	si ouinon(<§$0071§>)= non alors retour
      FIN
      
      
      i est un entier
      nligne est un entier
      
      
      Sablier(Vrai)
      JAUGE_TraitementCourant..Visible = Vrai
      JAUGE_TraitementCourant = 0
      
      // Cas multi archive
      si zipEstMulti(gMonArchive)=vrai ALORS
      	
      	si ouinon(<§$0084§>,<§$0085§>)=non ALORS retour
      	
      	// sauvegarde des lignes sélectionnées
      	ListeSupprimeTout(LISTE_CACHE)
      	pour i=1 _a_ TableOccurrence(TABLE_ARCHIVE)
      		si TABLE_ARCHIVE[i]..Sélectionnée ALORS
      			ListeAjoute(LISTE_CACHE,i)
      		FIN
      	FIN	
      	
      	// regroupe les multi-archives en une seule archive
      	RegroupeArchive()
      	
      	// restaure la sélection
      	pour i=1 _a_ ListeOccurrence(LISTE_CACHE)
      		TableSelectPlus(TABLE_ARCHIVE,LISTE_CACHE[i])
      	FIN
      FIN
      
      // cas particulier : si tout est sélectionné (plus rapide)
      si TableSelectOccurrence(TABLE_ARCHIVE)=TableOccurrence(TABLE_ARCHIVE) ALORS
      	// vidage de l'archive
      	zipSupprimeTout(gMonArchive)
      	TableSupprimeTout(TABLE_ARCHIVE)
      	// Etat des boutons
      	MajEtatBoutons()
      	
      	Sablier(faux)
      	JAUGE_TraitementCourant..Visible = faux
      	retour
      fin
      
      // On ajoute tous les fichiers à traiter
      POUR TOUTE LIGNE SELECTIONNEE nligne DE TABLE_ARCHIVE
      	si TABLE_ARCHIVE[nligne].COL_INDICE_ARCHIVE <> 0 ALORS
      		TableauAjoute(tabFichier,gMonArchive.Fichier[TABLE_ARCHIVE[nligne].COL_INDICE_ARCHIVE].Nom)		
      	FIN
      FIN
      
      // On passe le tableau en liste
      sListeFichier = TableauVersChaîne(tabFichier,RC)
      
      si zipSupprimeListeFichier(gMonArchive,sListeFichier,JAUGE_TraitementCourant)=0 ALORS
      	RafraichitAffichage()
      sinon
      	erreur("Erreur lors de la suppression des fichiers.  Erreur : "+rc+ ErreurInfo(errComplet))
      FIN
      
      Sablier(Faux)
      JAUGE_TraitementCourant..Visible = Faux
