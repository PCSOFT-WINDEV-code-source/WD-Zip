     code : |1+
      // Résumé : Ouvre le fichier sélectionné en l'extrayant dans le rép temporaire
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure Ouvrefichier()
      
      si TableSelectOccurrence(TABLE_ARCHIVE)=0 ALORS
      	erreur(<§$007f§>)
      	retour
      FIN
      
      // Début du traitement
      sablier(vrai)
      JAUGE_TraitementCourant..Visible =vrai
      JAUGE_TraitementCourant = 0
      
      sSauvRep est une chaine
      nResultat est un entier
      
      // sauvegarde du contexte
      sSauvRep = fRepEnCours()
      fRepCrée(fRépertoireTemp()+[fSep]+"Extraction")
      fRepEnCours(fRépertoireTemp()+[fsep]+"Extraction")
      
      //extraction
      nResultat = zipExtraitFichier(gMonArchive,COL_INDICE_ARCHIVE,zipAucun,JAUGE_TraitementCourant)
      
      // Mot de passe
      si nResultat = 21 ALORS
      	// Demande le mot de passe
      	sMotDePasse est une chaîne = ouvre(FEN_MotDePasse)		
      	si sMotDePasse <> "" ALORS
      		zipMotDePasse(gMonArchive,sMotDePasse)	
      		nResultat = zipExtraitFichier(gMonArchive,COL_INDICE_ARCHIVE,zipAucun,JAUGE_TraitementCourant)
      	FIN	
      fin
      
      
      si nResultat=0 ALORS
      	sGUID est une chaine = DonneGUID(guidBrut) + "_" 
      	fRenomme(fRépertoireTemp()+[fSep]+"Extraction"+fSep+COL_NOM,fRépertoireTemp()+[fSep]+"Extraction"+fSep+sGUID+COL_NOM)
      	LanceAppliAssociée(fRépertoireTemp()+[fSep]+"Extraction"+fsep+sGUID+COL_NOM)
      sinon
      	erreur(<§$0094§>, zipMsgErreur(nResultat))
      FIN
      
      // restauration du contexte
      fRepEnCours(sSauvRep)
      
      // Fin du traitement
      sablier(faux)
      JAUGE_TraitementCourant..Visible =Faux
