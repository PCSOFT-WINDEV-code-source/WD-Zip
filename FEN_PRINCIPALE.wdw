     name : GetIconeAssocié
     internal_properties : CAAAAAgAAAAzNcy9jVPQ12Ybr6n7raU1VQs5bNTWR1tc48F+c83iH9uJuf1q36Qc7iMhJ2l2LekLw6QW1Klh4tdAsi4kq2PUkldAKkKX54s6WH0CzMianczScfkgIn4yBXb7eT4LqT7y9Cogi17p2aa2ueQblqmYL6Fs8+CLgdLjlrbHYX6JSG+fhDn/ebH4q2Qj+8SwMl2fG+tFk9ZzGelKFe51zYFNfxrVUjbtvlXL3+u6ZsHOjXV8A/eWVqzpvxzEj7HJeEGDh1ZV8y3MQCTa7rv+oHy6EPosxw==
     procedure_id : 1175213845174462232
     type_code : 14
     code : |1-
      // Résumé : Renvoie l’icône associés au fichier passé en paramètre
      // Paramètres :
      // 	psFichier : fichier dont on doit retrouver l’icône
      // Valeur de retour :
      // 	chaîne ANSI : chemin vers l’icône
      //
      Procédure GetIconeAssocié(psFichier)
      // récupère l'icone associée à un fichier
      
      nIndiceIcone		est un entier
      nHandleIcone		est un entier = 1
      nDC				est un entier
      nHandleFichier	est un entier
      sFichier			est une chaine
      sIcone 			est une chaine
      
      // création d'un fichier temporaire avec la bonne extension
      sFichier = fRépertoireTemp()+psFichier
      
      nHandleFichier = fcree(sFichier)
      si nHandleFichier<>-1  ALORS
      	fferme(nHandleFichier)
      sinon
      	erreur(ErreurInfo)	
      fin
      
      // interrogation de l'icone associée
      nHandleIcone = AppelDLL32("Shell32.dll", "ExtractAssociatedIconA", SysInstance(), &sFichier, &nIndiceIcone)
      si nHandleIcone<>null alors
      	// Device Context du champ
      	nDC = dDébutDessin(IMG_ICONE)
      	si nDC<>0 alors
      		// Dessin de l'icône dans un champ image
      		AppelDLL32("User32.dll", "DrawIcon", nDC, 0, 0, nHandleIcone)
      	fin
      fin
      
      // sauvegarde de l’icône
      sIcone = dSauveImagePNG(IMG_ICONE, enMémoire,blanc)
      
      // Libération des ressources
      AppelDLL32("User32.dll", "DestroyIcon", nHandleIcone)
      
      // suppression du fichier temporaire
      fsupprime(sFichier)
      
      renvoyer sIcone
