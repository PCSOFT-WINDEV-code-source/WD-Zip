   -
     name : ExtractionFichiers
     procedure_id : 1602347548206350014
     type_code : 14
     code : |1+
      // Résumé : Extrait des fichiers d'une archive
      // Paramètres :
      //	sPourPilotage (chaîne ANSI - valeur par défaut="") :Chemin pour l'extraction des fichiers
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ExtractionFichiers(sPourPilotage est une chaine="")
      
      sDestination est une chaine
      nLigne est un entier
      sRetour est une chaine
      sAttribut est une chaine
      nAttribut est un entier
      bExtractGardeRep est un booleen
      bAttributNonTous, bAttributOuiTous sont des booleens
      tabFichier est un tableau de chaine
      
      
      // On vérifie que l'archive existe
      SI zipExiste(gMonArchive)=Faux ALORS
      	Erreur(<§@163ca2ca012797500000§>)
      	RETOUR
      FIN
      
      // Si aucun fichier n'est sélectionné
      SI TableSelectOccurrence(TABLE_ARCHIVE)=0 ALORS
      	SI OuiNon(<§@163ca2ca012797500008§>, <§@163ca2ca012797500009§>) ALORS
      		TableSelectPlus(TABLE_ARCHIVE)
      	SINON
      		RETOUR
      	FIN
      FIN
      
      
      // On sélectionne si besoin le répertoire d'extraction
      SI sPourPilotage="" ALORS
      	// Sélection d'un répertoire et mode d'extraction
      	sRetour = Ouvre(FEN_REPERTOIRE_EXTRACTION)
      SINON
      	sRetour = sPourPilotage
      FIN
      
      SI sRetour="" RETOUR
      
      // Répertoire
      sDestination = ComplèteRep(ExtraitChaîne(sRetour,1,TAB))
      // Mode
      bExtractGardeRep = ExtraitChaîne(sRetour,2,TAB)
      
      // Si aucun répertoire de destination
      SI sDestination="" ALORS
      	Erreur(<§@163ca2ca01279750000a§>)
      	RETOUR 
      FIN
      
      // extraction sans conservation de l'arborescence
      SI bExtractGardeRep=Faux
      	// Sauvegarde du répertoire courant
      	fRepEnCours(sDestination)
      FIN
      
      
      
      // Pour chaque ligne sélectionnée, on regarde si le fichier doit être ajouté à l'extraction
      POUR TOUTE LIGNE SELECTIONNEE nLigne DE TABLE_ARCHIVE
      	nIndiceArchive est un entier = TABLE_ARCHIVE[nLigne].COL_INDICE_ARCHIVE
      	
      	// On extrait pas le répertoire mais son contenu
      	si TABLE_ARCHIVE[nLigne].COL_ICONE.IMG_Fichier = IMG_Repertoire ALORS
      		pour tout FichierArchive de gMonArchive.Fichier
      			si contient(FichierArchive.Nom,TABLE_ARCHIVE[nLigne].COL_NOM) ALORS
      				// Le fichier est dans le répertoire, on l'extrait
      				TableauAjoute(tabFichier,FichierArchive.Nom)
      			FIN
      		FIN
      	FIN
      	
      	
      	SI bAttributOuiTous=Vrai ALORS
      		TableauAjoute(tabFichier,gMonArchive.Fichier[nIndiceArchive].Nom)
      	SINON
      		SI bAttributNonTous=Faux ALORS
      			
      			si nIndiceArchive = 0 ALORS
      				continuer
      			FIN
      			
      			sAttribut = gMonArchive.Fichier[nIndiceArchive].AttributFichier
      			nAttribut = Position(sAttribut, "R")+Position(sAttribut, "H")+Position(sAttribut, "S")
      			
      			SI nAttribut>0 ALORS
      				SELON Ouvre(FEN_OUINON, ChaîneConstruit(<§@163ca2ca01279750000b§>,gMonArchive.Fichier[nIndiceArchive].Nom))
      					CAS 1 // oui
      						TableauAjoute(tabFichier,gMonArchive.Fichier[nIndiceArchive].Nom)
      					CAS 2 // oui pour tous
      						bAttributOuiTous = Vrai
      						TableauAjoute(tabFichier,gMonArchive.Fichier[nIndiceArchive].Nom)
      					CAS 3 // non
      						
      					CAS 4 // non pour tous
      						bAttributNonTous = Vrai
      						
      					CAS 5 // annuler
      						RETOUR
      				FIN
      			SINON
      				TableauAjoute(tabFichier,gMonArchive.Fichier[nIndiceArchive].Nom)
      			FIN
      		FIN
      	FIN
      FIN
      
      // On garde les répertoires ? 
      nOption est un entier
      SI bExtractGardeRep = Vrai ALORS
      	nOption = zipRépertoire
      SINON
      	nOption = zipAucun
      FIN
      
      // Début de l'extraction
      Sablier(Vrai)
      JAUGE_TraitementCourant..Visible = Vrai
      JAUGE_TraitementCourant = 0
      
      // On extrait les fichiers
      ArchiveExtractionFichiers(tabFichier,sDestination,nOption,JAUGE_TraitementCourant)
      
      // Fin de l'extraction
      Sablier(Faux)
      JAUGE_TraitementCourant..Visible  = Faux
