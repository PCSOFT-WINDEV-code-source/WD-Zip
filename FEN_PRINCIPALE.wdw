     name : CréeArchive
     internal_properties : CAAAAAgAAADCMg9H+dFcwaCTznClHCHOVYAHOYU3UNWC4vAERzEOUzRcDDgtgAD4iM65qrqhq21e/51M/Vv9XGW42lGs4jLYG6+oTfR5hb1oRwlhH1svm2TvdJLilYYl/dKdHZp7H75cu9x4ySwUT1c6SP/uJEeqoZeTLoTpwOfi13aQ9wPtGEeJOZsExcq3Xwyeup/Hmjj4mz4X1j7onlN68Or1hKKvATxs5Dyl
     procedure_id : 1602347861740280697
     type_code : 14
     code : |1-
      // Résumé : Fonction qui permet de créer une archive
      // Paramètres :
      //	sFichierArchive (chaîne ANSI) :chemin de l'archive à créer
      // Valeur de retour :
      // 	booléen : Vrai si la création à réussit, faux sinon
      //
      PROCEDURE CréeArchive(sFichierArchive est une chaine) : booléen
      
      
      // Fermer l'éventuelle archive ouverte
      FermeArchive()
      
      // On vérifie si l'archive a bien été crée 
      si ArchiveCréeArchive(sFichierArchive) = faux ALORS
      	renvoyer faux
      FIN
      
      // On vérifie le format
      SI gMonArchive.FormatArchive=zipFormatTARGZ ALORS
      	Info(<§@163ca2ca012797500002§>)
      FIN
      
      // Chemin de l'archive courante
      gsArchive = sFichierArchive
      
      // On affiche les informations à l'utilisateur
      LIB_MESSAGE1 = ChaîneConstruit(<§@163ca2ca012797500003§>, 0, TailleVersChaîne(0))
      LIB_MESSAGE2 = ChaîneConstruit(<§@163ca2ca012797500004§>, 0, TailleVersChaîne(0))
      LIB_MESSAGE3 = <§@163ca2ca012797500005§>
      
      // On remplit les informations sur l'archive (en haut)
      LIB_CheminArchive = Gauche(gsArchive,Position(gsArchive,fSep,Taille(gsArchive),DepuisFin)-1)
      LIB_NomArchive = ExtraitChaîne(gsArchive,1,fSep,DepuisFin)
      
      // On change le titre de la fenêtre
      MaFenêtre..Titre = ProjetInfo(piNomProjet)+" - "+fExtraitChemin(sFichierArchive, fFichier+fExtension)
      
      // Etat des boutons
      MajEtatBoutons()
      
      // On accepte le drag and drop depuis l'explorateur
      ExplorerAccepte(Vrai,FEN_PRINCIPALE..Nom)
      
      
      renvoyer vrai
