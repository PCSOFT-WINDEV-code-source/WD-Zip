     name : AfficheUnFichierArchive
     internal_properties : CAAAAAgAAADmZ9dAt0RQRWpsXTSzJRfAUOfyr8Ihnfw1GVSYbE2IpeKdTTNDlS0s/egN5eyEJqeonTGyysoE+0owbVHY03JlaD5up+E/LlE+iZ8UPHOIDdL/lr1SZfpvvd1aEXzzAD8qdxmc89D5LFUQwtk9
     procedure_id : 1175213845174396696
     type_code : 14
     code : |1+
      // Résumé : Affiche un fichier présent dans l'archive à l'écran dans la table 
      // Paramètres :
      // 	nIndice : indice dans le fichier d'archive du fichier à afficher
      // Valeur de retour :
      // 	entier : Indice d'insertion
      //
      fonction AfficheUnFichierArchive(nIndice) : entier
      
      ////////////////////////////////////////////////////////////////////////////////////////
      // OPTIMISATION EN MODE REPERTOIRE, ON FAIT LES VERIFICATIONS EN PREMIER
      // PERMET DE GRANDEMENT ACCELERER LA VITESSE SI L ARCHIVE EST STRUCTURE EN DOSSIER
      ////////////////////////////////////////////////////////////////////////////////////////
      
      
      // On vérifie si il faut traiter le fichier
      sCheminFichier				est une chaîne
      sCheminFichier			= gMonArchive..Fichier[nIndice]..Nom
      sNomFichier					est une chaîne
      sNomFichier			= ExtraitChaîne(gMonArchive..Fichier[nIndice]..Nom,1,fSep,DepuisFin)
      
      // On est en mode répertoire ?
      SI gbAffichageModeRépertoire = Vrai ALORS
      	
      	SI ChaîneOccurrence(sCheminFichier,fSep) > gnNiveauAffichage ALORS
      		// Le répertoire existe déjà ?
      		sNomFichier = ExtraitChaîne(sCheminFichier,(gnNiveauAffichage+1),fSep,DepuisDébut)
      		nIndiceCherche est un entier = TableCherche(COL_NOM,sNomFichier)
      		SI nIndiceCherche>0 ALORS
      			RENVOYER nIndiceCherche
      		FIN
      	FIN
      	
      	// Le fichier ou répertoire contient le chemin courant? Si non il ne faut pas l'afficher
      	SI Contient(sCheminFichier,gsCheminRepertoireCourant) = Faux ET gsCheminRepertoireCourant<>"" ALORS
      		RENVOYER -1
      	FIN
      	
      	SI ChaîneOccurrence(sCheminFichier,fSep) < gnNiveauAffichage ALORS
      		// On affiche pas le fichier, car il est dans les dossiers précédents
      		RENVOYER -1
      	FIN	
      FIN
      
      
      
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      // FIN DES VERIFICATIONS / DEBUT ALGORITHME NORMAL
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      
      
      // Afficher le détail d'un fichier de l'archive courante
      
      sInfoFichier	 			est une chaine
      nTailleOriginale			est un entier
      nTailleCompressée			est un entier
      dDateHeureCreation			est un dateheure
      dDateHeureModification		est un dateheure
      dDateHeureAccès				est un dateheure
      sAttributFichier			est une chaine
      nTaux						est un entier
      sIcone						est une chaine
      nIndiceAjout 				est un entier
      
      sInfoFichier		= zipinfofichier(gMonArchive, nIndice)
      sNomFichier			= ExtraitChaîne(gMonArchive..Fichier[nIndice]..nom,1,fsep,DepuisFin)
      
      si sNomFichier="" renvoyer -1
      
      sCheminFichier			= gMonArchive..Fichier[nIndice]..Nom
      nTailleOriginale		= gMonArchive..Fichier[nIndice]..Taille
      nTailleCompressée		= gMonArchive..Fichier[nIndice]..TailleCompressée
      dDateHeureCreation		= gMonArchive..Fichier[nIndice]..DateCréation
      dDateHeureModification	= gMonArchive..Fichier[nIndice]..DateModification
      dDateHeureAccès			= gMonArchive..Fichier[nIndice]..DateDernierAccès
      sAttributFichier		= gMonArchive..Fichier[nIndice]..AttributFichier
      
      // Vérification des informations
      Si DateValide(dDateHeureCreation) = Faux 		alors dDateHeureCreation = DateSys()+HeureSys()
      Si DateValide(dDateHeureModification) = Faux 	alors dDateHeureModification = DateSys()+HeureSys()
      Si DateValide(dDateHeureAccès) = Faux 			alors dDateHeureAccès = DateSys()+HeureSys()
      
      // cas particulier des fichiers incompressibles
      si nTailleCompressée>nTailleOriginale ALORS
      	nTailleCompressée=nTailleOriginale
      FIN
      
      // calcul du taux de compression
      nTaux = nCalculeTaux(nTailleOriginale, nTailleCompressée)
      
      // récupère l'icone associée à un fichier
      sExtension est une chaine = fExtraitChemin(sNomFichier,fExtension)
      si gtaIcone[sExtension]..vide = Vrai ALORS
      	gtaIcone[sExtension] = GetIconeAssocié(sNomFichier)
      FIN
      
      sIcone = gtaIcone[sExtension]
      
      si gbAffichageModeRépertoire = vrai ALORS
      		
      	// On récupère le nom du fichier
      	sNomFichier = ExtraitChaîne(sCheminFichier,(gnNiveauAffichage+1),fsep,DepuisDébut)
      	
      	si ChaîneOccurrence(sCheminFichier,fsep) > gnNiveauAffichage ALORS
      		
      		// Le répertoire existe déjà ?
      		SI TableCherche(COL_NOM,sNomFichier)>0 ALORS
      			renvoyer TableCherche(COL_NOM,sNomFichier)
      		FIN
      		
      		// On affiche le répertoire du niveau courant			
      		nIndiceAjout = TableAjouteLigne(TABLE_ARCHIVE, nIndice, "", ...
      		sNomFichier,...
      		DateVersChaîne(dDateHeureModification..PartieDate)+" "+Gauche(HeureVersChaîne(dDateHeureModification..PartieHeure),5),...
      		"",... 
      		"", "", "",... 
      		DateVersChaîne(dDateHeureAccès..PartieDate)+" "+Gauche(HeureVersChaîne(dDateHeureAccès..PartieHeure),5),...
      		DateVersChaîne(dDateHeureAccès..PartieDate)+" "+Gauche(HeureVersChaîne(dDateHeureAccès..PartieHeure),5),...
      		sAttributFichier) 
      		
      		// On applique l'image au champ 
      		TABLE_ARCHIVE[nIndiceAjout].COL_ICONE.IMG_Fichier = IMG_Repertoire
      		
      		RENVOYER nIndiceAjout
      	FIN
      fin
      
      nIndiceAjout = TableAjouteLigne(TABLE_ARCHIVE, nIndice, "", ...
      sNomFichier,...
      DateVersChaine(dDateHeureModification..PartieDate)+" "+gauche(heureVersChaine(dDateHeureModification..Partieheure),5),...
      sCheminFichier,... 
      nTailleOriginale, nTailleCompressée, nTaux+"%",... 
      DateVersChaine(dDateHeureAccès..PartieDate)+" "+gauche(heureVersChaine(dDateHeureAccès..Partieheure),5),...
      DateVersChaine(dDateHeureAccès..PartieDate)+" "+gauche(heureVersChaine(dDateHeureAccès..Partieheure),5),...
      sAttributFichier)
      
      // On applique l'image au champ 
      TABLE_ARCHIVE[nIndiceAjout].COL_ICONE.IMG_Fichier = sIcone
      
      
      renvoyer nIndiceAjout
     type : 458752
