      // Résumé : Crée un fichier exe auto extractible 
      // Paramètres :
      //	psRepPilotage (valeur par défaut="") :Répertoire d'extraction
      // Valeur de retour :
      // 	Aucune
      //
      Procédure CréeAutoExtractible(psRepPilotage="")
      // Création de l'archive auto-extractible
      // psRepPilotage est rempli uniquement en mode pilotage
      
      // On vérifie si l'archive existe
      si zipExiste(gMonArchive)=Faux ALORS
      	erreur(<§$00a5§>)
      	retour
      FIN
      
      // si l'archive est multi-parties
      si zipestmulti(gMonArchive) alors
      	erreur(<§$00a6§>)
      	retour
      FIN
      
      // Si  l'option n'est pas autorisée
      SI gMonArchive.FormatArchive DANS (zipFormat7Z, zipFormatCAB, zipFormatRAR,zipFormatTAR,zipFormatTARGZ) alors
      	Erreur(<§$00aa§>)
      	RETOUR
      FIN
      
      // répertoire "Décompresser en" par défaut
      sRepExtraction est une chaine = psRepPilotage
      sRes est une chaine
      bIHM est un booleen
      
      // Si aucun répertoire 
      si sRepExtraction="" ALORS
      	sRes = ouvre(FEN_REPERTOIRE_EXTRACTION_DEFAUT)
      	si sRes="" retour	
      	sRepExtraction = ExtraitChaîne(sRes,1,tab)
      	bIHM = pas val(ExtraitChaîne(sRes,2,tab))
      	
      sinon
      	bIHM = faux
      FIN
      
      // Nom de l'exe
      sNomExe est une chaine
      sNomExe = fExtraitChemin(gsArchive,fDisque+fRépertoire+fFichier)+".exe"
      si fFichierExiste(sNomExe) ALORS
      	si ouinon(ChaîneConstruit(<§$0089§>,sNomExe),<§$008a§>)=non ALORS
      		retour
      	FIN
      FIN
      
      // Début du traitement
      sablier(vrai)
      JAUGE_TraitementCourant..Visible =vrai
      JAUGE_TraitementCourant = 0
      
      nRésultat est un entier
      si bIHM ALORS
      	nRésultat = zipCréeExe(gMonArchive, sNomExe, zipAvecIHM, "WDZip.ico", sRepExtraction,JAUGE_TraitementCourant)
      sinon
      	nRésultat = zipCréeExe(gMonArchive, sNomExe, zipRepDefaut+zipArborescence, "WDZip.ico", sRepExtraction,JAUGE_TraitementCourant)
      fin
      si nRésultat=0 ALORS
      	info(<§$0072§>,sNomExe)
      sinon
      	erreur(<§$0073§>, zipMsgErreur(nRésultat))	
      FIN
      
      // Fin du traitement
      sablier(faux)
      JAUGE_TraitementCourant..Visible =faux
