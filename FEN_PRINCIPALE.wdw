     code : |1+
      PROcedure fen_principale(gnNuméroLigneCommande est un entier=0)
      
      si pas enmodetest()
      	quand exception
      		erreur(ChaîneConstruit(<§$00a2§>,ProjetInfo(piNomProjet)),<§$00a3§>,"",<§$00a4§>,ExceptionInfo())
      		sablier(faux)
      		exceptionactive()
      		retour
      	FIN
      fin
      
      global
      	gfbExécutable est un booléen	// Booléen indiquant si on a ouvert un exécutable
      	gnNiveauAffichage est un entier = 0
      	gsCheminRepertoireCourant est une chaîne 
      	gtaIcone est un tableau associatif de chaine
      	
      local
      
      //MAJ Interface
      mafenetre..titre = ProjetInfo(piNomProjet)
      LIB_MESSAGE1 = ChaîneConstruit(<§$0077§>, 0, TailleVersChaîne(0))
      LIB_MESSAGE2 = ChaîneConstruit(<§$0078§>, 0, TailleVersChaîne(0))
      
      // Etat des boutons
      MajEtatBoutons()
      
      // Menu Ctx de la table
      CocheMenuCtx()
      
      // Drag and Drop depuis l'explorateur
      ExplorerAccepte(vrai,FEN_PRINCIPALE..Nom)
      Evénement("DragAndDrop", "*.", WM_DROPFILES)
      
      // Par défaut on est en mode répertoire
      INT_ModeRépertoire = vrai
      ExécuteTraitement(INT_ModeRépertoire,trtModification)
      
   -
     code : |1-
      // Selon le mode d'ouverture de la fenêtre principale
      si gnNuméroLigneCommande>0 ALORS
      	
