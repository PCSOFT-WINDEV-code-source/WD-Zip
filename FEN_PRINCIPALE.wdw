      Procédure Rechercher()
      // Rechercher un fichier dans une archive
      
      si zipExiste(gMonArchive)=Faux ALORS
      	erreur(<§$00a5§>)
      	retour
      FIN
      
      sFichierRecherché	est une chaine
      nIndiceFichier		est un entier
      nIndiceTable		est un entier
      
      // Sélectionner le fichier à rechercher
      sFichierRecherché = ouvre(FEN_RECHERCHE)
      
      si sFichierRecherché<>"" alors
      	
      	TableSelectMoins(TABLE_ARCHIVE)
      	
      	// Chercher un fichier dans l'archive en cours
      	nIndiceFichier = zipChercheFichier(gMonArchive, sFichierRecherché, faux)
      	si nIndiceFichier = -1 ALORS
      		info(chaineconstruit(<§$003e§>, sFichierRecherché))
      	sinon
      		nIndiceTable = TableCherche(TABLE_ARCHIVE.COL_INDICE_ARCHIVE,nIndiceFichier)
      		si nIndiceTable<>-1
      			TableSelectPlus(TABLE_ARCHIVE, nIndiceFichier)
      			TablePosition(TABLE_ARCHIVE, nIndiceFichier)
      		fin
      	fin
      fin
      
      
