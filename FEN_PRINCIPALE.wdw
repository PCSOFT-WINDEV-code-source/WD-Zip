     name : MajEtatBoutons
     internal_properties : CAAAAAgAAACjkYFYkPUVWXKvUS2nYurlRXz8vhxObQJQ1eT9zVw7p+cpuGRet4qpBfgrwa442HL4jFnaCAqgoegCj4E61lUFmi13CFYkwIXnjYODmkPd2PXfgXUYvD53bA7VOfL76dB4WVxA4sk4dTGYM7GnMBmTwpyETtFOWaObFIh4LaBPFNGJ7S+9p7imIENOQzHZETlf1SOCYO1wTh7groMwjCJMVdXt58D3mVsLYmA2BAZ5QHAuUy5/yp7mhnDeUtjPXyvv1LpYS7cqKw==
