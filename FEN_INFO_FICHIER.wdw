      si position(FEN_PRINCIPALE.TABLE_ARCHIVE.COL_attribut[pnLigneTable],"A")>0 ALORS
      	si sType<>"" alors sType+=", "
      	sType+=<§$0005§>
      fin
      si position(FEN_PRINCIPALE.TABLE_ARCHIVE.COL_attribut[pnLigneTable],"H")>0 ALORS
